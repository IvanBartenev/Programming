//Разработайте программу, которая будет печатать название числа (10-20), которое было
//введено пользователем(10 – десять, 11 – одиннадцать, 12 – двенадцать..)

#include<stdio.h>
#include<locale.h>
#include<math.h>


void main()
{
	setlocale(LC_ALL, "Russian");
	int a;
	puts("Введите число от 10 до 20(включительно):\n");
	scanf_s("%i", &a);
	switch (a)
	{
	case 10:
		puts("Ваше число: десять.");
		break;
	case 11:
		puts("Ваше число: одиннадцать.");
		break;
	case 12:
		puts("Ваше число: двенадцать.");
		break;
	case 13:
		puts("Ваше число: тринадцать.");
		break;
	case 14:
		puts("Ваше число: четырнадцать.");
		break;
	case 15:
		puts("Ваше число: пятнадцать.");
		break;
	case 16:
		puts("Ваше число: шестнадцать.");
		break;
	case 17:
		puts("Ваше число: семнадцать.");
		break;
	case 18:
		puts("Ваше число: восемьнадцать.");
		break;
	case 19:
		puts("Ваше число: девятнадцать.");
		break;
	case 20:
		puts("Ваше число: двадцать.");
		break;
	default:
		puts("Ошибка");
	}
}
