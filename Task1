void date()
{
	puts(" ____      ____       ____    ____       ____    .    .                            ");
	puts("     |    |          |    |       |     |    |   |    |                            ");
	puts("  ___|    |___       |    |   ____|     |    |   |____|                            ");
	puts(" |            |      |    |       |     |    |        |                            ");
	puts(" |____    ____|      |____|   ____|     |____|        |                            ");
}

void main()
{
	setlocale(LC_ALL, "RUS");
	puts("Моя программа");
	puts("Hello World");
	puts("* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *");
	puts("*                                                                                 *");
	puts("* тема: Написание и компиляция консольного приложения в среде разработки MVS 2022 *");
	puts("*       Выполнил: Бартенев Иван Дмитриевич, бИСТ - 222                            *");
	puts("*                                                                                 *");
	puts("* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *");
	puts("");
